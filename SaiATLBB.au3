	ToolTip($TEXT, $X, $Y)
EndFunc


Func ISSHOWTESTINFO()
	Local $STATUS = GUICtrlRead($SHOWTESTINFO_CHECKBOX)
	If $STATUS = 1 Then
		$TESTINFOSTATUS = $STATUS
		Return 1
	EndIf
	If $STATUS = 4 And $TESTINFOSTATUS <> 4 Then
		$TESTINFOSTATUS = $STATUS
		ToolTip("", 0, 0)
	EndIf
	Return 0
EndFunc


