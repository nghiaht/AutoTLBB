Func DOWNDANGERPOS()
	$DANGERPOS -= 7
	If $DANGERPOS < 1 Then $DANGERPOS = 1
	SETDANGERPOSINPUT($DANGERPOS)
EndFunc


Func CHECKDANGERPOS()
