Func _IETABLEGETCOLLECTION(ByRef $O_OBJECT, $I_INDEX = -1)
	If Not IsObj($O_OBJECT) Then
		__IEERRORNOTIFY("Error", "_IETableGetCollection", "$_IEStatus_InvalidDataType")
		SetError($_IESTATUS_INVALIDDATATYPE, 1)
